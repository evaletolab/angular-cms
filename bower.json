{
  "name": "angular-cms",
  "version": "0.1.0",
  "homepage": "https://github.com/evaletolab/angular-cms",
  "authors": [
    "Olivier Evalet <evaleto@karibou.ch>"
  ],
  "description": "simple way to embed your github documentation in your angular application",
  "main": [
    "dist/ng-cms.js",
    "dist/ng-cms.css"
  ],
  "keywords": [
    "cms",
    "markdown",
    "angular"
  ],
  "license": "GPL",
  "ignore": [
    "node_modules",
    "bower_components",
    "test",
    "demo",
    "src",
    "tests"
  ],
  "overrides": {
    "showdown": {
      "main": "src/showdown.js"
    }
  },
  "dependencies": {
    "angular": "~1",
    "angular-route": "~1",
    "angular-sanitize": "~1",
    "angular-resource": "~1",
    "showdown": "~0.3.1",
    "bootstrap": "~3.2.0",
    "underscore": "~1.7.0"
  },
  "devDependencies": {
    "angular-mocks": "latest"
  }
}
